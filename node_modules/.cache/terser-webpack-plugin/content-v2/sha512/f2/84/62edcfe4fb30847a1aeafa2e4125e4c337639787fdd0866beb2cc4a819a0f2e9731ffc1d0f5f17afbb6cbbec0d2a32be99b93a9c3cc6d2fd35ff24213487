 fields')\n            setName('');\n            setAge('');\n           }\n       else{\n        let input = {\n            name: name,\n            age : age\n        }\n        setAlert(false);\n        setMessage('Added sudcesfully')\n        setName('');\n        setAge('');\n        props.onAdd(input);\n\n       }\n    }\n\n    const onCloseHandler = () => {\n        setAlert(false);\n    }\n\n    return (\n        <div className='userform'>\n            <form className='form-controls' onSubmit={onSubmitHandler}>\n                <div className=''>\n                    <label htmlFor='name'>User Name</label>\n                    <input type='text' id='name' onChange={(e)=> setName(e.target.value)} value={name}/>\n                </div>\n                <div>\n                    <label htmlFor='age'>Age</label>\n                    <input type='number' id='age' onChange={(e)=> setAge(e.target.value)} value={age}/>\n                </div>\n                <button className='btn btn-add'>Add User</button>\n            </form>\n            \n            {alert ? <Alert message={message} alert={alert} onClose = {onCloseHandler}/>  : ''}\n        </div>\n    );\n};\n\nexport default UserForm;","import React from 'react';\nimport { useState } from 'react';\nimport './User.css'\nimport UserForm from './UserForm';\nimport UserList from './UserList';\n\nconst User = (props) => {\n    const [list, setList] = useState([]);\n\n     const onAddHAndler = (ip) => {\n            setList([...list, {...ip, id: Math.random().toString()} ])\n\n            console.log('list is', list);\n     }\n\n    return (\n        <div className='user'>\n                <h3>Add Your Info</h3>\n                <UserForm onAdd={(ip)=>onAddHAndler(ip)}/>  \n                {list.map((lis)=>{\n                    return <UserList list={lis} key={lis.name}/> \n                })}\n        </div>\n    );\n};\n\nexport default User;","import React from 'react';\nimport User from './components/User/Us